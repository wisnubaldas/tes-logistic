{
    "openapi": "3.0.0",
    "info": {
        "title": "Test Logistic",
        "description": "REST API PT X ",
        "contact": {
            "name": "wisnubaldas"
        },
        "version": "1.0"
    },
    "servers": [
        {
            "url": "http://localhost:8000",
            "description": "Api Logistic Backend"
        }
    ],
    "paths": {
        "/api": {},
        "/api/login": {
            "post": {
                "tags": [
                    "Auth"
                ],
                "summary": "User Login",
                "description": "Login dengan user account untuk mendapatkan auth token agar aplikasi tetap terhubung gunakan token pada header ",
                "operationId": "signin",
                "requestBody": {
                    "description": "Dibutuhkan login untuk memulai session aplikasi",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "email": {
                                        "description": "User email",
                                        "type": "string"
                                    },
                                    "password": {
                                        "description": "User password",
                                        "type": "string"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "ok",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "success": {
                                            "type": "boolean"
                                        },
                                        "data": {
                                            "type": "object",
                                            "example": {
                                                "token": "13|Y7jXDOh4dAnAEreE0QcP4BGnWPjgTftkXOaON9ct",
                                                "name": "Admin"
                                            }
                                        },
                                        "message": {
                                            "type": "string",
                                            "example": "User signed in"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Unauthorized"
                    }
                }
            }
        },
        "/api/logout": {
            "post": {
                "tags": [
                    "Auth"
                ],
                "summary": "User Login",
                "operationId": "logout",
                "responses": {
                    "200": {
                        "description": "Sukses logout",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "success": {
                                            "type": "string"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Unauthorized"
                    }
                },
                "security": [
                    {
                        "apiAuth": []
                    }
                ]
            }
        },
        "/api/barang": {
            "get": {
                "tags": [
                    "Barang"
                ],
                "summary": "Get all barang",
                "operationId": "getAllBarang",
