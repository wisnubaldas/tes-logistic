    "components": {
        "schemas": {
            "Barang": {
                "title": "Barang model",
                "description": "Barang model",
                "properties": {
                    "satuan_id": {
                        "description": "id satuan",
                        "type": "string",
                        "format": "satuan_id"
                    },
                    "kd_barang": {
                        "description": "kode barang",
                        "type": "string",
                        "format": "kd_barang"
                    },
                    "nama": {
                        "description": "nama barang",
                        "type": "string",
                        "format": "nama"
                    },
                    "harga": {
                        "description": "harga barang",
                        "type": "string",
                        "format": "harga"
                    },
                    "qr": {
                        "description": "qr barang",
                        "type": "string",
                        "format": "qr"
                    },
                    "qty": {
                        "description": "quantity barang",
                        "type": "string",
                        "format": "qty"
                    }
                },
                "type": "object"
            },
            "Report": {
                "title": "Report model",
                "description": "Report model",
                "properties": {
                    "id": {
                        "description": "id report",
                        "type": "integer",
                        "format": "id"
                    },
                    "r_date": {
                        "description": "tanggal report",
                        "type": "string",
                        "format": "r_date"
                    },
                    "tot_trans": {
                        "description": "total transaksi per hari",
                        "type": "integer",
                        "format": "tot_trans"
                    },
                    "tot_order": {
                        "description": "total order per hari",
                        "type": "integer",
                        "format": "tot_order"
                    },
                    "detail": {
                        "description": "detail json string dari transaksi",
                        "type": "object",
                        "format": "detail"
                    }
                },
                "type": "object"
            },
            "Transaksi": {
                "title": "Transaksi model",
                "description": "Transaksi model",
                "properties": {
                    "id": {
                        "description": "id transaksi",
                        "type": "integer",
                        "format": "id"
                    },
                    "order_id": {
                        "description": "id order",
                        "type": "integer",
                        "format": "order_id"
                    },
                    "customer_id": {
                        "description": "id customer",
                        "type": "integer",
                        "format": "customer_id"
                    },
                    "barang_id": {
                        "description": "id barang",
                        "type": "integer",
                        "format": "harga"
                    },
                    "owner_name": {
                        "description": "nama pembeli",
                        "type": "string",
                        "format": "owner_name"
                    },
                    "cardbank_type": {
                        "description": "tipe kartu kredit",
                        "type": "string",
                        "format": "cardbank_type"
                    },
                    "card_number": {
                        "description": "nomer kartu kredit",
                        "type": "string",
                        "format": "card_number"
                    },
                    "payment_status": {
                        "description": "status pembayaran",
                        "type": "string",
                        "format": "payment_status"
                    },
                    "total": {
                        "description": "total barang yang dibeli",
                        "type": "integer",
                        "format": "total"
                    },
                    "order_valid_date": {
                        "description": "tanggal valid transaksi",
                        "type": "string",
                        "format": "order_valid_date"
                    },
                    "order_valid_time": {
                        "description": "jam valid transaksi",
                        "type": "time",
                        "format": "order_valid_time"
                    }
                },
                "type": "object"
            }
        },
        "securitySchemes": {
            "apiAuth": {
                "type": "http",
                "description": "Login with email and password to get the authentication token",
                "name": "Token based Based",
                "in": "header",
                "bearerFormat": "JWT",
                "scheme": "bearer"
            }
        }
    },
    "tags": [
